@model SpartanClash.ViewModels.CompanyRegistry
@{
    ViewBag.Title = "ServiceRecord";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1><u>Service Record</u></h1>

<div class="searchArea">
    <div id="adv-search" class="input-group">
        <input class="form-control" placeholder="Search or select a company..." type="text" />
        <div class="input-group-btn">
            <div class="btn-group" role="group">
                <button class="btn btn-primary" type="button">
                    <span class="glyphicon glyphicon-search" aria-hidden="true">
                    </span>
                </button>
            </div>
        </div>
    </div>
</div>
<h3> </h3>


<div class="allCompanies">

    @foreach (SpartanClash.ViewModels.CompanyRegistryItem item in Model.registryItems)
    {

        {
            <div class="leaderboardItem">
                <span class="companyName">@Html.ActionLink(item.companyName, "CompanyResults/" + item.companyName, "ServiceRecord")</span>
            </div>

            <p></p>
        }
    }
    



</div>


